<resources>

    <!--Values-vxx之间的差异属性-->
    <style name="CcdAppTheme" parent="CustomAppTheme">
        <item name="android:windowBackground">@drawable/icon_bg_common</item>
    </style>

    <style name="SplashTheme" parent="CcdAppTheme">
        <item name="android:windowBackground">@drawable/laterlist_splash</item>
    </style>

    <!--popwindow动画-->
    <style name="popwindow_anim_style" parent="CustomAppTheme">
        <item name="android:windowEnterAnimation">@anim/anim_enter_bottom</item>
        <item name="android:windowExitAnimation">@anim/anim_exit_bottom</item>
    </style>
    <!--备注选中-->
    <style name="green_check_box" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@null</item>
        <item name="android:background">@drawable/selector_green_check_box</item>
    </style>

    <style name="order_find_tab_text_size" parent="TextAppearance.AppCompat.Widget.ActionBar.Title.Inverse">
        <item name="android:textSize">14sp</item>
    </style>
</resources>